<
{
	"name": "<%= it.engine.name %>",
	"scopeName": "text.html.<%= it.engine.id %>",
	"injectionSelector": "L:text.html",
	"patterns": [<% if (it.prefix.comment) { -%>
		{
			"include": "#tag-block-comment"
		},
<% } -%>
		{
			"include": "#tag-single-line-section-comment"
		},
		{
			"include":"#tag-section-comment"
		},
		{
			"include":"#tag-<%= it.engine.id %>-single-line"
		},
		{
			"include":"#tag-<%= it.engine.id %>-multi-line"
		}
	],
	"repository": {<% if (it.prefix.comment) { -%>
		"tag-block-comment": {
			"begin": "<%= it.tag.open.plain %><%= it.prefix.comment %>",
			"beginCaptures": {
				"0": {
					"name": "punctuation.definition.comment.<%= it.engine.id %>"
				}
			},
			"end": "<%= it.tag.close %>",
			"endCaptures": {
				"0": {
					"name": "punctuation.definition.comment.<%= it.engine.id %>"
				}
			},
			"name": "comment.block.<%= it.engine.id %>"
		},
		<% } %>
		"tag-single-line-section-comment": {
			"begin": "(<%= it.tag.open.withPrefix %>)\\s*((\/\\*)(.*)(\\*\/))(?=(\\s*(<%= it.tag.close.withWsControl %>)))",
			"captures": {
				"1": {
					"name": "keyword.operator.relational.js"
				},
				"3": {
					"name": "punctuation.definition.comment.<%= it.engine.id %>"
				},
				"5": {
					"name": "punctuation.definition.comment.<%= it.engine.id %>"
				},
				"2": {
					"name": "comment.block.<%= it.engine.id %>"
				}
			},
			"end": "([%?]>)",
			"endCaptures": {
				"1": {
					"name": "keyword.operator.relational.js"
				}
			},
			"contentName": "comment.block.<%= it.engine.id %>",
			"name":"meta.block.single.comment.<%= it.engine.id %>"
		},
		"tag-section-comment": {
			"name":"comment.block.<%= it.engine.id %>",
			"begin":"(<[%?](?:(?!php)))\\s*((?=(\/\\*)))",
			"beginCaptures": {
				"1": {
					"name": "keyword.operator.relational.js"
				}
			},
			"end": "(\\*\/)\\s*([%?]>)",
			"endCaptures": {
				"1": {
					"name": "punctuation.definition.comment.<%= it.engine.id %>"
				},
				"2": {
					"name": "keyword.operator.relational.js"
				}
			},
			"patterns":[
				{
					"name":"<%= it.engine.id %>-end-tag",
					"match":"([%?]>)",
					"captures":{
						"1":{
							"name":"keyword.operator.relational.js"
						}
					}
				},
				{
					"name":"<%= it.engine.id %>-begin-tag",
					"match":"(<[%?](?:(?!php)))",
					"captures":{
						"1":{
							"name":"keyword.operator.relational.js"
						}
					}
				},
				{
					"include":"text.html.<%= it.engine.id %>"
				}
			]
		},
		"tag-<%= it.engine.id %>-single-line": {
			"name": "meta.tag.metadata.script.<%= it.engine.id %>",
			"begin":"(<[%?](?:(?!php))[%_=-]?)(?!(\\s*\\*\/))(((?!([_-]?[%?]>)).)+)(?=([_-]?[%?]>))",
			"beginCaptures": {
				"0": {
					"contentName":"source.js",
					"name": "meta.embedded.*"
				},
				"1": {
					"name": "keyword.operator.relational.js"
				},
				"3": {
					"name": "meta.embedded.<%= it.engine.id %>",
					"contentName": "source.js",
					"patterns":[
						{
							"include":"source.js"
						}
					]
				},
				"6": {
					"name": "keyword.operator.relational.js"
				}
			},
			"end": "(([_-]?[%?]>))",
			"endCaptures": {
				"1": {
					"name": "keyword.operator.relational.js"
				}
			}
		},
		"tag-<%= it.engine.id %>-multi-line": {
			"contentName":"source.js",
			"name": "meta.block.<%= it.engine.id %>",
			"begin":"(<[%?](?:(?!php))[%_=-]?)(?!(\\s*\\*\/))",
			"beginCaptures": {
				"1": {
					"name": "keyword.operator.relational.js"
				},
				"3": {
					"name": "meta.embedded.<%= it.engine.id %>",
					"contentName": "source.js",
					"patterns":[
						{
							"include":"source.js"
						}
					]
				}
			},
			"end": "(([_-]?[%?]>))",
			"endCaptures": {
				"1": {
					"name": "keyword.operator.relational.js"
				}
			},
			"patterns":[
				{
					"include":"source.js"
				}
			]
		}
	}
}
